{
  "name": "webpack-seed",
  "version": "1.0.0",
  "description": "Webpack seed for development",
  "main": "webpack.config.js",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ]
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/francomaaanz/webpack-seed.git"
  },
  "keywords": [
    "webpacks",
    "seed",
    "es6",
    "sass"
  ],
  "author": "Franco Manzur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/francomaaanz/webpack-seed/issues"
  },
  "homepage": "https://github.com/francomaaanz/webpack-seed#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^4.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
